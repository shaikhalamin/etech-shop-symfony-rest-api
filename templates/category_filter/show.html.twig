{% extends 'base.html.twig' %}

{% block title %}CategoryFilter{% endblock %}

{% block body %}
    <h1>CategoryFilter</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ category_filter.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ category_filter.name }}</td>
            </tr>
            <tr>
                <th>Created_at</th>
                <td>{{ category_filter.createdAt ? category_filter.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Updated_at</th>
                <td>{{ category_filter.updatedAt ? category_filter.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_category_filter_index') }}">back to list</a>

    <a href="{{ path('app_category_filter_edit', {'id': category_filter.id}) }}">edit</a>

    {{ include('category_filter/_delete_form.html.twig') }}
{% endblock %}
